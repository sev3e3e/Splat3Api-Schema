    "license": "MIT",
    "private": false,
    "main": "./dist/splat3api-types/index.ts",
    "scripts": {
        "generate": "openapi-generator-cli generate -i ./src/schema.json -g typescript-fetch -o ./dist/splat3api-types --additional-properties=supportsES6=true,typescriptThreePlus=true,npmName=splat3api-types",
        "build": "yarn clean && yarn generate",
        "clean": "rimraf dist/splat3api-types/src"
    },
    "devDependencies": {
        "@openapitools/openapi-generator-cli": "^2.5.2",
        "@types/node": "^18.14.6",
        "rimraf": "^4.3.0",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.5"
    },
    "version": "0.0.1-alpha"
}
